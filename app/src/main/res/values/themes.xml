<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GoGreenApplication" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/GoGreenWhite</item>
        <item name="colorPrimaryVariant">@color/DarkGreen</item>
        <item name="colorOnPrimary">@color/lightGreen</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/CoralBlue</item>
        <item name="colorSecondaryVariant">@color/DarkCoralBlue</item>
        <item name="colorOnSecondary">@color/GoGreenWhite</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorOnPrimary</item>
        <!-- Customize your theme here. -->
    </style>
</resources>